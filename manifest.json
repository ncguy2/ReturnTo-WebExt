{
	"manifest_version": 2,
	"name": "ReturnTo",
	"version": "0.1.3",
	"default_locale": "en",
	"description": "Stores a page element for later",
	"icons": {
		"48": "icons/ReturnTo_tmp.png"
	},

	"sidebar_action": {
		"default_icon": "icons/ReturnTo_tmp.png",
		"default_title" : "Return to",
		"default_panel": "sidebar/panel.html"
	},
	
	"applications": {
		"gecko": {
			"id": "{731795b3-d901-4672-a708-33f8f21004f4}"
		}
	},

	"background": {
		"scripts": ["shared/Global.js", "background.js"]
	},

	"options_ui": {
		"page": "settings/options.html"
	},

	"permissions": ["<all_urls>", "activeTab", "storage", "contextMenus", "tabs"]

}